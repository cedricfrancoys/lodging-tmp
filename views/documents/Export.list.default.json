{
    "name": "Exports list",
    "description": "Complete list of exports, most recent on top.",
    "access": {
        "groups": ["booking.default.user"]
    },
    "order": "created",
    "sort": "desc",
    "domain": ["center_office_id", "in", "user.center_offices_ids"],
    "header": {
        "actions": {
            "ACTION.CREATE" : false
        }
    },
    "controller": "lodging_documents_export-collect",
    "layout": {
        "items": [
            {
                "type": "field",
                "value": "name",
                "width": "15%",
                "sortable": true
            },
            {
                "type": "field",
                "value": "created",
                "width": "15%",
                "readonly": true,
                "sortable": true
            },
            {
                "type": "field",
                "label": "Type",
                "value": "export_type",
                "width": "15%",
                "sortable": true,
                "readonly": true
            },
            {
                "type": "field",
                "label": "Downloaded?",
                "value": "is_exported",
                "width": "15%",
                "sortable": true,
                "readonly": true
            }
        ]
    }
}